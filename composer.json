{
  "name": "uhin/ccda-parser-library",
  "description": "A library providing data objects to interact with Consolidated Clinical Document Architecture (C-CDA) data.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Mark Chavez",
      "email": "mchavez@uhin.org"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "fzaninotto/faker": "^1",
    "php-mock/php-mock-phpunit": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Uhin\\Ccda\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Uhin\\Ccda\\Tests\\": "tests/"
    }
  }
}
